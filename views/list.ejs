<%- include("header") -%>
        <div class="box" id="heading">
            <h1><%=listTitle%></h1>
        </div>
        <div class="box">
            <% for(item in newListItems) { %>
                <div class="item">
                    <input type="checkbox">
                    <p><%=newListItems[item]%></p>
                </div> 
            <%} %>
            <form class="item" action="/" method="post">
                <input type="text" placeholder="Enter Task" name="newItem" autocomplete="off">
                <button type="submit" name="list" value=<%= listTitle %>>+</button>
            </form>  
        </div>
<%- include("footer") -%>